<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <!--
        Added the constraints for entity RpmAction.
    -->
    <changeSet id="20210925191502-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="plan_id"
                                 baseTableName="rpm_action"
                                 constraintName="fk_rpm_action__plan_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rpm_plan"/>

        <addForeignKeyConstraint baseColumnNames="reason_id"
                                 baseTableName="rpm_action"
                                 constraintName="fk_rpm_action__reason_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rpm_reason"/>

        <addForeignKeyConstraint baseColumnNames="captures_id"
                                 baseTableName="rpm_action"
                                 constraintName="fk_rpm_action__captures_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rpm_capture"/>

        <addForeignKeyConstraint baseColumnNames="result_id"
                                 baseTableName="rpm_action"
                                 constraintName="fk_rpm_action__result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rpm_result"/>

        <addForeignKeyConstraint baseColumnNames="project_id"
                                 baseTableName="rpm_action"
                                 constraintName="fk_rpm_action__project_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rpm_project"/>
    </changeSet>
</databaseChangeLog>
